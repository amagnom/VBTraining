Public Class Calculadora

    Public Function Somar(ByVal valorUm As Integer, ByVal valorDois As Integer) As Integer
        Return valorUm + valorDois
    End Function
    Function Subtrair(ByVal valorUm As Double, ByVal valorDois As Double) As Double
        Return valorUm - valorDois
    End Function

    Function Dividir(ByVal valorUm As Double, ByVal valorDois As Double) As Double
        Return valorUm / valorDois
    End Function

    Function Multiplicar(ByVal valorUm As Double, ByVal valorDois As Double) As Double
        Return valorUm * valorDois
    End Function


End Class
